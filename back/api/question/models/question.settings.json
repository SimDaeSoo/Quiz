{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "questions",
  "info": {
    "name": "Question"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "content": {
      "type": "text"
    },
    "score": {
      "type": "integer",
      "default": 1
    },
    "quiz": {
      "model": "quiz",
      "via": "questions"
    },
    "Answer": {
      "type": "boolean",
      "required": true,
      "private": false
    },
    "explain": {
      "type": "text"
    }
  }
}
